#include<bits/stdc++.h>
using namespace std;

typedef struct s{
	string a;
} adam;

int main() {
	int N;
	cin >> N;
	adam dam[N];
	int output[N];
	for (int i=0;i<N;i++)
	{
		string a;
		cin >> a;
		a= dam[i];
		int flag=0;
		output[i]=0;
		for (int j=0;j<i;j++)
		{
			if(strcmp(dam[i],dam[j])>0)
				output[i]++;
		}
	}
	for (int i=0;i<N;i++)
	{
		cout << output[i] << endl;
	}
	}


